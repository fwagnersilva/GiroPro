# Ambiente
NODE_ENV=development

# Servidor
PORT=3000

# Banco de Dados
# Para desenvolvimento local: deixe vazio para usar SQLite
# Para produção (Render): use a URL do PostgreSQL
DATABASE_URL=

# Autenticação
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_refresh_secret_here

# Logs
LOG_LEVEL=debug

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
FRONTEND_URL=http://localhost:5173
