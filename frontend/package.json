{
  "name": "react-native-boilerplate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "badge:remove": "node scripts/add-icon-badge.js remove",
    "postinstall": "node scripts/postinstall.mjs",
    "build:web": "expo export --platform web --output-dir dist",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild:web": "rimraf .expo node_modules/.cache",
    "postbuild:web": "ls -la dist",
    "serve:web": "npx serve web-build -s -n -p ${PORT:-8081}",
    "clean": "rimraf .expo android coverage ios",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "prepare": "husky install || true",
    "badge:add": "node scripts/add-icon-badge.js add dev",
    "badge:add:staging": "node scripts/add-icon-badge.js add staging",
    "badge:add:beta": "node scripts/add-icon-badge.js add beta"
  },
  "overrides": {
    "react-test-renderer": "19.1.0"
  },
  "dependencies": {
    "@dev-plugins/react-query": "^0.3.1",
    "@expo/config-plugins": "^54.0.2",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@shopify/flash-list": "^2.1.0",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "expo": "^54.0.12",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-image": "^3.0.9",
    "expo-linking": "~8.0.4",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "graphql": "^16.8.1",
    "i18next": "^25.5.3",
    "lodash.memoize": "^4.1.2",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "^1.19.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-reanimated": "^4.1.3",
    "react-native-restart": "^0.0.27",
    "react-native-worklets": "0.6.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "^0.21.0",
    "react-query-kit": "^3.3.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "3.4.18",
    "babel-plugin-module-resolver": "^5.0.0",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
